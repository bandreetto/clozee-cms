{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Eventos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "banner": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "startAt": {
      "type": "datetime",
      "required": true
    },
    "endAt": {
      "type": "datetime",
      "required": true
    },
    "posts": {
      "type": "component",
      "repeatable": true,
      "component": "clozee-api.post"
    }
  }
}
